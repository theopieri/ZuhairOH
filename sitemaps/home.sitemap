sitemap home label="Home"
{
  Frame label="Home Overview" {
    Switch item=masterOFFAllLights mappings=[ON="All ON", OFF="All OFF"]
    //Switch item=masterOFFGFLights mappings=[ON="All ON", OFF="All OFF"]
    //Switch item=masterOFFBSLights mappings=[ON="All ON", OFF="All OFF"]
