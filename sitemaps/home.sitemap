    //Switch item=masterOFFGFBlinds mappings=[UP="UP", DOWN="DOWN"]  
    //Switch item=masterOFFBSBlinds mappings=[UP="UP", DOWN="DOWN"]  
    Switch item=masterOFFAllAC mappings=[OFF="All OFF"]   
    //Switch item=masterOFFGFAC mappings=[OFF="All OFF"]   
    //Switch item=masterOFFBSAC mappings=[OFF="All OFF"]   
    Switch item=masterOFFAllUF mappings=[ON="All ON",OFF="All OFF"]   
    //Switch item=masterOFFGFUF mappings=[OFF="All OFF"]   
    //Switch item=masterOFFBSUF mappings=[OFF="All OFF"]
    Switch item=hotwater mappings=[ON="Run 1Hour"]
    Switch item=recwater mappings=[ON="Run 2 mins"]
